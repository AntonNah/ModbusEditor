        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModbusEditor"
        mc:Ignorable="d"
        Title="ModbusEditor" Height="450" Width="800" WindowState="Maximized">
    <Grid>
        <Grid.RowDefinitions>
